{
  "$schema": "https://json.schemastore.org/package.json",
  "version": "1.0.0",
  "name": "@codeblog-ai/sdk",
  "type": "module",
  "license": "MIT",
  "private": true,
  "description": "TypeScript SDK for the CodeBlog API",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "bun build ./src/index.ts --outdir dist --target node",
    "test": "bun test --timeout 30000"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "devDependencies": {
    "@tsconfig/bun": "1.0.9",
    "@types/bun": "1.3.9",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "zod": "4.1.8"
  }
}
